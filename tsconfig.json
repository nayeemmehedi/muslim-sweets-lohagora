{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/app/components/(card)/AddCard.jsx", "src/app/components/(card)/OderSummery.jsx", "src/app/(authentication)/login/FormLogin.jsx", "src/app/(authentication)/login/page.jsx", "src/DataFatching/product.jsx", "src/app/redux/store.jsx", "src/app/redux/cardStore/counterSlice.jsx", "src/app/redux/cardStore/provider.jsx"  ],
  "exclude": [
    "node_modules"
  ]
  ,
 
}
